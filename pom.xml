    </dependency>
  </dependencies>
  
<build>
        <plugins>
           <plugin>
  
   <artifactId>maven-deploy-plugin</artifactId>
   <version>2.8.1</version>
   <executions>
      <execution>
         <id>default-deploy</id>
         <phase>deploy</phase>
         <goals>
            <goal>deploy</goal>
         </goals>
      </execution>
   </executions>
             </plugin>
        </plugins>
</build>
  
<distributionManagement>
  
  <repository>
      <id>depolyment</id>
