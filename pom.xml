      <url>https://osca-wbench.psst.t-online.corp/nexus/content/repositories/releases/</url>
   </repository>
  
  
   <snapshotRepository>
      <id>depolyment</id>
      <name>Snapshot repo</name>
      <url>https://osca-wbench.psst.t-online.corp/nexus/content/repositories/snapshots/</url>
